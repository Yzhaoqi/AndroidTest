<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yzq.com.androidtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity_luanch_mode.ActivityTestA"
            android:launchMode="singleTask" />
        <activity
            android:name=".activity_luanch_mode.ActivityTestB"
            android:launchMode="singleTop" />
        <activity android:name=".activity_service.ActivityTestService" />

        <service
            android:name=".activity_service.TestService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".activity_broadcast.ActivityTestBroadcast" />
        <activity android:name=".activity_async.ActivityTestAsync" />
        <activity android:name=".activity_shared_preference.ActivityTestSharedPreference" />
        <activity android:name=".activity_recycler_view.ActivityRecyclerView" />
        <activity android:name=".activity_view_action.ActivityTestViewAction"></activity>
    </application>

</manifest>